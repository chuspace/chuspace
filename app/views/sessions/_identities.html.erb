<% Identity.providers.each_key do |provider| %>
  <%= button_to "/auth/#{provider}", class: "flex items-center text-white justify-center mt-2 w-full button #{login_button_colors_classes[provider].join(' ')} text-center", data: { turbo: false }, form: { class: 'w-full' } do %>
    <%= inline_svg_tag "icons/#{provider}", class: 'fill-current' %> <span class='ml-2'><%= provider.humanize %></span>
  <% end %>
<% end %>
