<% git_provider_users.each do |user|%>
  <li tabindex="0" role="menuitemradio">
    <a class='flex items-center' data-menu-button-contents>
      <%= image_tag user.avatar_url, class: 'w-4 h-4 mask mask-circle mr-2' if user.avatar_url %>
      <%= content_tag :span, user.username %>
    </a>
  </li>
<% end %>
